{
  "tasks": {
    "run": "deno run --allow-read=.env,.env.defaults --allow-net main.ts",
    "compile": "deno compile --allow-read=.env,.env.defaults --allow-net --output spirit_box_bot main.ts",
    "deploy": "scp spirit_box_bot do:/srv/spirit_box_bot.new",
    "docker:build": "docker build -t spirit_box_bot .",
    "docker:run": "docker run --rm -v ./.env:/opt/.env spirit_box_bot"
  }
}
